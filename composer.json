{
  "name": "webphplove/queue",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "debug",
    "queue"
  ],
  "description": "queue for swoft",
  "license": "Apache-2.0",
  "require": {
    "swoft/console": "^1.0",
    "swoft/framework": "^1.0",
    "swoft/data-parser": "^1.0",
    "swoft/swoole-ide-helper": "dev-master",
    "swoft/redis": "^1.0",
    "swoft/process": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "webphplove\\Queue\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "post-package-update": "echo 'Please rerun command: php bin/swoft dev:publish swoft/devtool -f'"
  }
}